<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.1.0</Version>
    <Authors>Golois Mouelet</Authors>
    <Description>Using Entity Framework Core at the background, automatically plug and play this package to do basic CRUD operation with less configuration</Description>
    <PackageProjectUrl>https://github.com/mgolois/automaticentity</PackageProjectUrl>
    <RepositoryUrl>https://github.com/mgolois/automaticentity</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="2.2.6" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="2.2.6" />
  </ItemGroup>

</Project>
